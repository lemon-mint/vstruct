enum Speices {
    human,
    elf,
    orc,
    dwarf,
    gnome,
    halfling,
    halfElf,
    halfOrc,
    dragonborn,
    tiefling,
    gnoll,
    goblin
}

struct Coordinate {
    int64 x;
    int64 y;
}

enum ItemType {
    weapon,
    armor,
    potion
}

struct Item {
    ItemType type;
    string name;

    int64 damage;
    int64 armor;
}

struct Inventory {
    Item rightHand;
    Item leftHand;
}

alias UUID = string;

struct Entity {
    UUID id;
    Speices type;
    Coordinate position;
    int64 hp;

    Inventory inventory;
}

service TestService {
    Coordinate InitCoordinate();
    Coordinate Move(Coordinate position);
    Item NewItem(ItemType type, string name, int64 damage, int64 armor);
}
